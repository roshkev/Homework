
public class Domasni {

	public static void main(String[] args) {
		int[] myArray = new int[] { 1, 4, 8, 3, 5, 8 , 8 };
		
		int sum = 0;
		for (int i = 0; i < myArray.length; i++)
		for (int row = 0; row < myArray.length; row++){
			
			if (myArray[i] % 2 == 0);
				

			sum = sum + myArray[i];
		
			System.out.println("Сумата на четните числа в масива е:" + sum);

		}
		}


	}


