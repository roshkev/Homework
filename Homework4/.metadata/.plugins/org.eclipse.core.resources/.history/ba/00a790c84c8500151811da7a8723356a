import java.util.Scanner;

public class work1 {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);

		System.out.println("Molia vavedete proizvolno tcislo");

		int n = scanner.nextInt();

		
		int counter = 0;
		
	    while (counter <= n)

		{
		for (int i = 1; i < n; i++);
		
		
		{

			System.out.println(counter);
		}
		counter++;
	}

}
}