package drugi;

public class vreme {

	public static void main(String[] args) {
		// Declare some variables
		byte centuries = 21;
		short years = 2000;
		int days = 730480;
		long hours = 17531520;
		// Print the result on the console
		System.out.println(centuries + " centuries is " + years +
				" years, or " + days + " days, or " + hours + " hours.");

	}

}
