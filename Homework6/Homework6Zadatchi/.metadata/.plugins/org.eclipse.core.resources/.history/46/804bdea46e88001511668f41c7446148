
public class DomashaZadatcha1 {

	public static void main(String[] args) {

		int Arr5[] = { 12, 34, 56, 78, 6, 5, 5, 45, 4, 3 };

		int br = 0;

		for (int I = 0; I < Arr5.length; I++)

			if ((Arr5[I] % 2) == 0)
				br++;

		System.out.println("Броят на четните елементи в масива е: " + br);

	}

}