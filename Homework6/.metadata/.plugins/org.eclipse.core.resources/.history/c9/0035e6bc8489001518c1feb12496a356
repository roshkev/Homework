
public class DomashnaZadatcha4 {

	public static void main(String[] args) {

		int[] myArray = new int[] { 2, 7, 9, 3, 9, 8, 7, 6 };

		int n = 9;

		for (int row = 0; row < myArray.length; row++) {
			row = n;
			System.out.println("Индекса на първото намерено число е: " + row);
			break;

		}
	}
}