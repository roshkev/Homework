
public class DomashnoZadatca1 {

	public static void main(String[] args) {

		int[] myArray = new int[] { 2, 6, 7, 9 };

		for (int i = 0; i < myArray.length; i++) {
			for (int row = 0; row < myArray.length; row++)
			if (i % 2 == 1) {
				continue;
			}

			System.out.println("Tcetnite tcisla sa: " + i );

		}

	}

}
