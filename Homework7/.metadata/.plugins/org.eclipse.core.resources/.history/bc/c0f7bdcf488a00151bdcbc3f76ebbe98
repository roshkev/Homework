
public class DomashnaZadatcha3Urok7 {

	public static void main(String[] args) {

		int[][] Matrix =

		{ { 1, 1, 2,  }, { 10, 8, 9,  }, { 12, 7, 9,  } };

		for (int row = 0; row < Matrix.length; row++) {

			for (int col = 0; col < Matrix.length; col++) {
				if (Matrix[row][col] % 2 == 0) {
					System.out.println(Matrix[row][col]);


				}
			}
		}
	}
}
